<script type="text/javascript">
  var connected_flag = 0
  var mqtt;
  var reconnectTimeout = 20000;
  var host = "mqtt.smeird.com";
  var port = 8083;
  var clean = 0;
  var obj = 0.001;
  var Diff;

  // Create a Client instance
  Client = new Paho.MQTT.Client(host, port, uuidv4());

  // set callback handlers
  Client.onConnectionLost = onConnectionLost;
  Client.onMessageArrived = onMessageArrived;


  // Connect the client, with a Username and Password
  Client.connect({
    onSuccess: onConnect,
    useSSL: true
  });

  function uuidv4() {
    return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {
      var r = Math.random() * 16 | 0,
        v = c == 'x' ? r : (r & 0x3 | 0x8);
      return v.toString(16);
    });
  }



  // called when the Client connects
  function onConnect() {
    // Once a connection has been made, make a subscription and send a message.
    console.log("INdex Page Data Connect");
    Client.subscribe("Observatory/#");
    document.getElementById("connected").innerHTML = "Live";
  }

  // called when the Client loses its connection
  function onConnectionLost(responseObject) {
    if (responseObject.errorCode !== 0) {
      console.log("onConnectionLost:" + responseObject.errorMessage);
      document.getElementById("connected").innerHTML = responseObject.errorMessage;
    }
  }

  // called when a message arrives
  function onMessageArrived(message) {
    if (message !== null) {
      console.log("onMessageArrived:" + message.payloadString + '  | ' + message.destinationName);

      // document.getElementById("energy").innerHTML = message.payloadString;
      //   document.getElementById("returned_energy").innerHTML = message.payloadString("input");
      //  var obj = JSON.parse(message.payloadString);

      if (message.destinationName == "Observatory/roof/openlimit") {

        if (message.payloadString == 0) {
          document.getElementById("OLS").classList.add('bg-success');
          document.getElementById("OLS").classList.remove('bg-danger');
        } else {
          document.getElementById("OLS").classList.remove('bg-success');
          document.getElementById("OLS").classList.add('bg-danger');
        }
      }

      if (message.destinationName == "Observatory/roof/closelimit") {

        if (message.payloadString == 0) {
          document.getElementById("CLS").classList.add('bg-success');
          document.getElementById("CLS").classList.remove('bg-danger');
        } else {
          document.getElementById("CLS").classList.remove('bg-success');
          document.getElementById("CLS").classList.add('bg-danger');
        }
      }

      //document.getElementById("OutTemp").innerHTML = dp(obj.outTemp_C);
      if (message.destinationName == "Observatory/clouds") {
        document.getElementById("clouds").innerHTML = message.payloadString;
        if (message.payloadString < -12) {
          document.getElementById("clouds_S").classList.add('border-left-success');
          document.getElementById("clouds_S").classList.remove('border-left-danger');
          document.getElementById("clouds_T").classList.add('text-success');
          document.getElementById("clouds_T").classList.remove('text-danger');
          document.getElementById("clouds_I").classList.add('fa-check');
          document.getElementById("clouds_I").classList.remove('fa-times');
          document.getElementById("cond_I").classList.add('fa-moon');
          document.getElementById("cond").innerHTML = 'Its Clear'
        } else {
          document.getElementById("clouds_S").classList.remove('border-left-success');
